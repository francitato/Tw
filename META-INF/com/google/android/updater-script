				ui_print("Installing Divine Beats...");
run_program("/sbin/busybox", "mount", "/system");
run_program("/sbin/busybox", "mount", "/data");
run_program("/sbin/busybox", "mount", "/vendor");
delete_recursive("/system/priv-app/ViPER4Android");
delete_recursive("/system/priv-app/ViPER4Android");
package_extract_dir("Arianoxx/Divine/system", "/system");
package_extract_dir("Arianoxx/Divine/vendor", "/vendor");
#mount("yaffs2", "MTD", "modextras", "/extras");
#package_extract_dir("Arianoxx/Divine/extras", "/extras");
package_extract_file("Arianoxx/Divine/buildPropbak.sh", "/tmp/buildPropbak.sh");
set_perm(0, 0, 0777, "/tmp/buildPropbak.sh");
run_program("/tmp/buildPropbak.sh");
delete("/tmp/buildPropbak.sh");
package_extract_file("Arianoxx/Divine/runme.sh", "/tmp/runme.sh");
set_perm(0, 0, 0777, "/tmp/runme.sh");
run_program("/tmp/runme.sh");
delete("/tmp/runme.sh");
package_extract_dir("Arianoxx/Divine/DivineBeats", "/tmp");
package_extract_file("Arianoxx/Divine/divinebeats.sh", "/tmp/divinebeats.sh");
set_perm(0, 0, 0777, "/tmp/divinebeats.sh");
run_program("/tmp/divinebeats.sh");
delete("/tmp/divinebeats.sh");
package_extract_file("Arianoxx/Divine/deepbuffer.sh", "/tmp/deepbuffer.sh");
set_perm(0, 0, 0777, "/tmp/deepbuffer.sh");
run_program("/tmp/deepbuffer.sh");
delete("/tmp/deepbuffer.sh");
package_extract_file("Arianoxx/Divine/dolby.sh", "/tmp/dolby.sh");
set_perm(0, 0, 0777, "/tmp/dolby.sh");
run_program("/tmp/dolby.sh");
delete("/tmp/dolby.sh");
set_perm(0, 2000, 0755, "/system/etc/init.d/batsav");
set_perm_recursive(0, 0, 0755, 0644, "/system/app/DolbyMobileAudioEffectService");
set_perm_recursive(0, 0, 0755, 0755, "/system/addon.d");
set_perm_recursive(0, 2000, 0755, 0755, "/system/bin");
endif;



