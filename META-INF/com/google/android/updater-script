set_progress(0.10);
#--PREPARE DEVICE PARTITIONS
ui_print("-- Preparing device partitions");
package_extract_dir("META-INF/SCRIPTS", "/tmp");
run_program("/sbin/mount", "-t", "auto", "/system");
run_program("/sbin/mount", "-t", "auto", "/data");
delete_recursive("/system");
#--AROMA OPTIONS
set_progress(0.20);
if file_getprop("/tmp/aroma/opt.prop", "item.1.1") == "1" then 
ui_print("-- Backing up efs partition");
set_metadata("/tmp/efs_backup.sh", "uid", 0, "gid", 0, "mode", 0777);
run_program("/tmp/efs_backup.sh");
else
ui_print("-- Continue without efs backup");
endif;
if file_getprop("/tmp/aroma/opt.prop", "item.1.2") == "1" then 
ui_print("-- Wiping all user data files");
set_metadata("/tmp/wipe.sh", "uid", 0, "gid", 0, "mode", 0777);
run_program("/tmp/wipe.sh");
else
ui_print("-- Continue without safe format");
endif;
if file_getprop("/tmp/aroma/opt.prop", "item.1.3") == "1" then 
ui_print("-- Cleaning up all device partitions");
delete_recursive("/system");
delete_recursive("/sdcard");
delete_recursive("/data");
delete_recursive("/cache");
delete_recursive("/preload");
else
ui_print("-- Continue without hard format");
endif;
